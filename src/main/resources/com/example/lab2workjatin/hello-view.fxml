<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab2workjatin.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Label fx:id="user_name" text="User name" />
  <TextField fx:id="eid" text="id" />
  <TextField fx:id="ename" text="name" />
  <TextField fx:id="esalary" text="salary" />
  <TextField fx:id="eaddress" text="address" />
  <TextField fx:id="eage" text="age" />

  <Label fx:id="welcomeText" />
  <Button onAction="#onHelloButtonClick" text="Click Here" />
  <Button mnemonicParsing="false" onAction="#InsertData" text="Insert" />
  <Button mnemonicParsing="false" onAction="#DeleteData" text="Delete" />
  <Button mnemonicParsing="false" onAction="#UpdateData" text="Update" />
  <Button mnemonicParsing="false" onAction="#LoadData" text="Load" />
  <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
    <columns>
      <TableColumn fx:id="id" prefWidth="75.0" text="id" />
      <TableColumn fx:id="employee_name" prefWidth="75.0" text="employee_name" />
      <TableColumn fx:id="salary" prefWidth="75.0" text="Salary" />
      <TableColumn fx:id="address" prefWidth="75.0" text="Address" />
      <TableColumn fx:id="age" prefWidth="75.0" text="Age" />
    </columns>
  </TableView>
</VBox>
